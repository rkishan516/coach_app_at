  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Int",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main.dart",
      "args": [
        "--dart-define-from-file",
        ".env/int.json"
      ]
    },
    {
      "name": "Debug Web Int",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main.dart",
      "args": [
        "--web-port",
        "5000",
        "--web-browser-flag",
        "--dart-define-from-file",
        ".env/int.json",
      ]
    },
    {
      "name": "Profile Int",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main.dart",
      "args": [
        "--dart-define-from-file",
        ".env/int.json"
      ]
    },
    {
      "name": "Release Int",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main.dart",
      "args": [
        "--dart-define-from-file",
        ".env/int.json"
      ]
    },
    {
      "name": "Debug Prod",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "prod",
        "--dart-define-from-file",
        ".env/prod.json"
      ]
    },
    {
      "name": "Release Prod",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "prod",
        "--dart-define-from-file",
        ".env/prod.json"
      ]
    }
  ]
}